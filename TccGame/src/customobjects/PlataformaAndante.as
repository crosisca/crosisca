package  customobjects{		import citrus.objects.platformer.box2d.MovingPlatform;			public class PlataformaAndante extends MovingPlatform {				private var xInicial:int;		private var yInicial:int;				private var xFinal:int;		private var yFinal:int;				private var velocidade:int;				private var frequencia:int;		private var esperarPorAlavanca:Boolean = false;				private var sprite:String;		private var friccao:int;				public function PlataformaAndante(name:String, params:Object) {			params.view = sprite;			params.enabled = esperarPorAlavanca;			params.speed = velocidade;			params.startX = xInicial;			params.startY = yInicial;			params.endX = xFinal;			params.endY = yFinal;			params.fricction = friccao;			super(name, params);		}				public function activateObjectsByFrequency(freq:int):void		{			if(freq == frequencia)				this.enabled = true;		}	}	}