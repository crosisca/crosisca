package  customobjects{	import citrus.objects.platformer.box2d.Platform;	import com.greensock.TweenNano;			public class Portao extends Platform {				public var direcao:String;		public var centoEOitenta:Boolean = false;		public var velocidade:int;		public var tempo:int;		public var sprite:String;		public var frequencia:int;				public function Portao(name:String, params:Object) {			params.view = sprite;			super(name, params);		}				public function activateObjectsByFrequency(freq:int)		{			var toRotate:int = 90;			if(freq == frequencia){				if(centoEOitenta)					toRotate = 180;				if(direcao == "e")					toRotate = toRotate*-1;								TweenNano.to(this, tempo, {rotation:toRotate, ease:Linear.easeNone});			}		}			}	}